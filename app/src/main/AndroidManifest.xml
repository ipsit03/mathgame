<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        tools:targetApi="31">
        <activity
            android:name=".gameactivity3"
            android:exported="false"
            android:label="@string/title_activity_gameactivity3"
            android:theme="@style/Theme.Mathga" />
        <activity
            android:name=".gameactivity2"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.gamepage" />
        <activity
            android:name=".resultactivity"
            android:exported="false"
            android:theme="@style/Base.Theme.result" />
        <activity
            android:name=".gameactivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.gamepage" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Mathga">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>